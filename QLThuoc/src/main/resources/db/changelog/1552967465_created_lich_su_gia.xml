			referencedColumnNames="id" referencedTableName="san_pham" />
		<addForeignKeyConstraint baseColumnNames="chi_nhanh_id" baseTableName="lich_su_gia"
			constraintName="fk_chi_nhanh_id_lich_su_gia" deferrable="true" initiallyDeferred="true" onDelete="CASCADE" onUpdate="RESTRICT"
			referencedColumnNames="id" referencedTableName="chi_nhanh" />
	</changeSet>
</databaseChangeLog>