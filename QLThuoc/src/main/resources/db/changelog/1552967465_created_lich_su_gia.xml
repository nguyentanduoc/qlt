			</column>
			<column name="chi_nhanh_id" type="bigint">
				<constraints nullable="false"/>
			</column>
			<column name="ngay_thay_doi" type="time">
				<constraints nullable="false"/>
			</column>
			<column name="don_gia" type="double">
