		</createTable>
	</changeSet>
	<changeSet id="02" author="ntduoc">
		<loadData catalogName="cat" encoding="UTF-8"
			file="db/changelog/csv/1548987025_roles.csv"
			schemaName="public"  tableName="quyen">
			<column name="ten_quyen" type="STRING" />
			<column name="mo_ta" type="STRING" />
			<column name="cap_do" type="NUMERIC" />
