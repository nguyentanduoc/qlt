<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
	<changeSet id="01" author="ntduoc">
		<createTable tableName="badges">
			<column name="id" type="SERIAL">
				<constraints nullable="false" unique="true"
					primaryKey="true" />
			</column>
			<column name="variant" type="varchar(255)">
				<constraints nullable="false" />
			</column>
			<column name="text" type="varchar(255)">
				<constraints nullable="false" />
			</column>
		</createTable>
	</changeSet>
	<changeSet id="02" author="ntduoc">
		<loadData catalogName="cat" encoding="UTF-8"
			file="db/changelog/csv/1549894488_badges.csv" schemaName="public"
			tableName="badges">
			<column name="variant" type="STRING" />
			<column name="text" type="STRING" />
		</loadData>
	</changeSet>
</databaseChangeLog>