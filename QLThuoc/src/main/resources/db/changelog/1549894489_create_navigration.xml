	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
	<changeSet id="add_sequence" author="ntduoc">
		<createSequence sequenceName="SEQ_DEFAULT_NAV" cycle="false" minValue="1" maxValue="9223372036854775807"
			startValue="100" incrementBy="1" />
	</changeSet>
	<changeSet id="01" author="ntduoc">
		<createTable tableName="navigration">
			<column name="id" type="bigint" defaultValueComputed="nextval('SEQ_DEFAULT_NAV')">
				<constraints nullable="false" unique="true" primaryKey="true" />
			</column>
			<column name="badge_id" type="bigint">
				<constraints nullable="true" />
			</column>
			<column name="name" type="varchar(255)">
				<constraints nullable="false" unique="true"/>
			</column>
			<column name="url" type="varchar(255)">
				<constraints nullable="true" unique="true"/>
			</column>
			<column name="icon" type="varchar(255)">
				<constraints nullable="true" />
			</column>
			<column name="title" type="boolean">
				<constraints nullable="true" />
			</column>
			<column name="sort_num" type="bigint">
				<constraints nullable="false" unique="true"/>
			</column>
			<column name="has_children" type="boolean">
				<constraints nullable="false" />
			</column>
			<column name="is_children" type="boolean">
				<constraints nullable="false" />
			</column>
			<column name="parent_id" type="bigint">
				<constraints nullable="true" />
			</column>
		</createTable>
		<addForeignKeyConstraint baseColumnNames="badge_id" baseTableName="navigration"
			constraintName="fk_navigration_badges" deferrable="true" initiallyDeferred="true" onDelete="CASCADE" onUpdate="RESTRICT"
			referencedColumnNames="id" referencedTableName="badges" />
		<addForeignKeyConstraint baseColumnNames="parent_id" baseTableName="navigration"
			constraintName="fk_parent_nav" deferrable="true" initiallyDeferred="true" onDelete="CASCADE" onUpdate="RESTRICT"
			referencedColumnNames="id" referencedTableName="navigration" />
	</changeSet>
	<changeSet id="02" author="ntduoc">
		<loadData catalogName="cat" encoding="UTF-8" file="db/changelog/csv/1549894489_navigrations.csv" schemaName="public"
			tableName="navigration">
