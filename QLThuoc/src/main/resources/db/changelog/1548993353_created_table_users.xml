<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
	<changeSet id="01" author="ntduoc">
		<createTable tableName="tai_khoan">
			<column name="id" type="int">
				<constraints nullable="false" unique="true"
					primaryKey="true" />
			</column>
			<column name="ten_tai_khoan" type="varchar(255)">
				<constraints nullable="false" />
			</column>
			<column name="ten_dang_nhap" type="varchar(255)">
				<constraints nullable="false" unique="true" />
			</column>
			<column name="email" type="varchar(255)">
				<constraints nullable="false" unique="true" />
			</column>
			<column name="mat_khau" type="varchar(255)">
				<constraints nullable="false" />
			</column>
			<column name="hoat_dong" type="boolean">
				<constraints nullable="false" />
			</column>
			<column name="created_at" type="date" defaultValueComputed="CURRENT_TIMESTAMP">
				<constraints nullable="true" />
			</column>
			<column name="updated_at" type="date" defaultValueComputed="CURRENT_TIMESTAMP">
				<constraints nullable="true" />
			</column>
		</createTable>
	</changeSet>
</databaseChangeLog>